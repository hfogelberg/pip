{
  "name": "pip",
  "description": "The book about Little Pip",
  "version": "1.0.0",
  "author": "Henrik Fogelberg <henrik.gogelberg@gmail.com>",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently --kill-others \"npm run vue\" \"npm run server\"",
    "server": "cross-env NODE_ENV='development' nodemon server.js",
    "vue": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "cloudinary": "^1.9.1",
    "cors": "^2.8.4",
    "mongoose": "^4.13.6",
    "validator": "^9.2.0",
    "vue": "^2.4.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "now": {
    "alias": "pip"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
